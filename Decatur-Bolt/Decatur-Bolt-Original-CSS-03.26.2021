/*++++++++++++++++++++++++++ FIXED NAVIGATION +++++++++++++++++++++++++*/

#main-navbar .navbar-nav>li>a {
	font-family: Montserrat, sans-serif;
	padding: 17px 13px;
	font-size: 12px;
	letter-spacing: 1px;
	text-decoration: none;
	font-weight: 700;


	color: #777777;
	text-shadow: 0 1px 0 #ffffff;
}
#main-navbar .navbar-nav>li>a:hover {
	background-color: #e7e7e7;
}
.navbar-fixed-top {
	-webkit-box-shadow: 0 2px 3px 0 rgba(0, 0, 0, .3);
	box-shadow: 0 2px 3px 0 rgba(0, 0, 0, .3);
	background: #fefefe;
	/* Old browsers */
    /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top, #fefefe 1%, #fbfbfb 100%);
	/* FF3.6+ */
    
	background: -webkit-gradient(linear, left top, left bottom, color-stop(1%, #fefefe), color-stop(100%, #fbfbfb));
	/* Chrome,Safari4+ */
    
	background: -webkit-linear-gradient(top, #fefefe 1%, #fbfbfb 100%);
	/* Chrome10+,Safari5.1+ */
    
	background: -o-linear-gradient(top, #fefefe 1%, #fbfbfb 100%);
	/* Opera 11.10+ */
    
	background: -ms-linear-gradient(top, #fefefe 1%, #fbfbfb 100%);
	/* IE10+ */
    
	background: linear-gradient(to bottom, #fefefe 1%, #fbfbfb 100%);
    /* W3C */
    
    filter: progid: DXImageTransform.Microsoft.gradient( startColorstr='#fefefe', endColorstr='#fbfbfb', GradientType=0);
	/* IE6-8 */
    
	border-top: 2px solid rgba(79, 145, 205, 1);
}
.transparent #main-navbar .navbar-nav>li>a {
	color: #efefef;
	text-shadow: 0px 0px 0px #ffffff;
}
.transparent #main-navbar .navbar-nav>li>a:hover {
	background-color: rgba(255, 255, 255, .2);
}
.navbar-fixed-top.transparent {
	border-bottom: 1px solid rgba(153, 153, 153, 0);
	color: rgba(153, 153, 153, 0);
	;
	-webkit-box-shadow: 0 2px 3px 0 rgba(0, 0, 0, .0);
	box-shadow: 0 2px 3px 0 rgba(0, 0, 0, .0);
	background: transparent;
	border-top: 2px solid rgba(245, 121, 31, 0);
	border-color: rgba(245, 121, 31, 0);
}
