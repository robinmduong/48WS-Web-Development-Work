}
}
/*++++++ Floating Newsletter Signup ++++++*/


#floating_link, #newsletter-signup {
	-webkit-transition: all .5s ease-out;
	-moz-transition: all .5s ease-out;
	-ms-transition: all .5s ease-out;
	-o-transition: all .5s ease-out;
	transition: all .5s ease-out;
}
#floating_link {
	position: fixed;
	left: -86px;
	top: 400px;
	display: block;
	width: 200px;
	height: 33px;
	/* text-indent: -10000px; */
	overflow: hidden;
	background-color: #337ab7;
	color: #ffffff;
	text-align: center;
	font-weight: bold;
	letter-spacing: 1px;
	text-transform: uppercase;
	padding-top: 4px;
	border: 1px solid #303030;
	border-radius: 3px;
	-moz-transform: rotate(90deg);
	-webkit-transform: rotate(90deg);
	-o-transform: rotate(90deg);
	-ms-transform: rotate(90deg);
	transform: rotate(90deg);
	text-decoration: none;
	z-index: 900;
}
#floating_link:hover {
	-webkit-box-shadow: 2px 2px 5px 0 rgba(0,0,0,.4);
	box-shadow: 2px 2px 5px 0 rgba(0,0,0,.4);
}
#newsletter-signup {
	position: fixed;
	left: -390px;
	top: 230px;
	display: block;
	width: 400px;
	height: 440px;
	background-color: #fff;
	border-radius: 10px;
	border: 1px solid #ddd;
	padding: 20px;
	color: #303030;
}
body.home-page #newsletter-signup, body.home-page #floating_link {
	position: absolute;
}
body.home-page #newsletter-signup {
	top: 800px;
}
body.home-page #floating_link {
	top: 1000px
}
#newsletter-signup.form-open {
	left: -10px;
	-webkit-box-shadow: 1px 4px 7px 2px rgba(0,0,0,.3);
	box-shadow: 1px 4px 7px 2px rgba(0,0,0,.3);
}
#floating_link.form-open {
	left: 300px;
}

